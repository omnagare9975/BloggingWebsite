{
  "family": "FinalDEST",
  "executionRoleArn": "arn:aws:iam::108782100108:role/allowECSToAccessECR",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "awsnode",
      "image": "placeholder",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
